<catalog type="photos">
<photo customer ID= "donatedBy" (name, description,date, images?) >
	<!ATTLIST photo custID ID #REQUIRED >
<!--	<!ATTLIST donatedBy CDATA >?-->


	<!ELEMENT name (#PCDATA)>
	<!ATTLIST name metadata >

	<!ELEMENT description (#PCDATA)>
	<!ELEMENT date (#PCDATA)>

	<!ELEMENT images (img+)>
	<!ELEMENT img EMPTY>
	<!ATTLIST img(src) >

	
	
	
</photo>
